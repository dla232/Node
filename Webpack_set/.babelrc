{
  "presets" : [
    "@babel/preset-env",
    "@babel/react",
  ],
  "plugins": [
    "syntax-async-functions",
    "@babel/plugin-transform-async-to-generator",
    "@babel/plugin-transform-regenerator",
    "@babel/plugin-transform-runtime",
    ["babel-plugin-root-import", {
      "rootPathPrefix": "~",
      "rootPathSuffix": "src"
    }]
  ],
}